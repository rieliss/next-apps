generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

model details {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  v           Int?      @map("__v")
  createdAt   DateTime? @db.Date
  description String
  status      Boolean
  title       String
  updatedAt   DateTime? @db.Date
}
