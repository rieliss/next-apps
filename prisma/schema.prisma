generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

model details {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  description String
  status      Boolean @default(false)
  title       String
}

model firstdate {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  description String
  status      Boolean @default(false)
  title       String
  time String
}

model seconddate {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  description String
  status      Boolean @default(false)
  title       String
  time String
}

model thirddate {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  description String
  status      Boolean @default(false)
  title       String
  time String
}
